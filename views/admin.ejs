<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        <title>서비스관리자</title>

        <!-- Bootstrap CSS -->
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65"
            crossorigin="anonymous"
        />

        <!-- Jquery -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

        <link rel="stylesheet" href="./css/main.css" />
        <link rel="stylesheet" href="./css/tabmenu.css" />
        <noscript><link rel="stylesheet" href="./css/noscript.css" /></noscript>
    </head>
    <body class="is-preload">
        <!-- Wrapper -->
        <div id="wrapper">
            <%- include('header.ejs') %>
            <!-- Main -->
            <div id="main">
                <div class="inner">
                    <h1>회원관리실</h1>
                    <div class="col">
                        <div class="row">
                            <div class="col-sm-5" id="filter-content">
                                <input
                                    id="filter-value"
                                    type="text"
                                    class="form-control"
                                    placeholder="검색"
                                />
                            </div>
                        </div>
                    </div>
                    <section class="ftco-section">
                        <div class="container1">
                            <ul class="tabs">
                                <li class="tab-link current" data-tab="tab-1">전체회원</li>
                                <li class="tab-link" data-tab="tab-2" id="customer">일반회원</li>
                                <li class="tab-link" data-tab="tab-3" id="partner">파트너회원</li>
                                <li class="tab-link" data-tab="tab-4" id="waiting">승인대기</li>
                            </ul>
                            <div id="tab-1" class="tab-content current">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-wrap">
                                            <table class="table">
                                                <thead class="thead-primary">
                                                    <tr>
                                                        <th>번호</th>
                                                        <th>이름</th>
                                                        <th>성별</th>
                                                        <th>아이디</th>
                                                        <th>연락처</th>
                                                        <th>주소</th>
                                                        <th>가입일</th>
                                                        <th>비고</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="userList" id="allUserList"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="tab-2" class="tab-content">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-wrap">
                                            <table class="table">
                                                <thead class="thead-primary">
                                                    <tr>
                                                        <th>번호</th>
                                                        <th>이름</th>
                                                        <th>성별</th>
                                                        <th>아이디</th>
                                                        <th>연락처</th>
                                                        <th>주소</th>
                                                        <th>가입일</th>
                                                        <th>비고</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="userList" id="customUserList"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="tab-3" class="tab-content">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-wrap">
                                            <table class="table">
                                                <thead class="thead-primary">
                                                    <tr>
                                                        <th>번호</th>
                                                        <th>이름</th>
                                                        <th>성별</th>
                                                        <th>아이디</th>
                                                        <th>연락처</th>
                                                        <th>주소</th>
                                                        <th>가입일</th>
                                                        <th>비고</th>
                                                    </tr>
                                                </thead>
                                                <tbody
                                                    class="userList"
                                                    id="partnerUserList"
                                                ></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="tab-4" class="tab-content">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-wrap">
                                            <table class="table">
                                                <thead class="thead-primary">
                                                    <tr>
                                                        <th>번호</th>
                                                        <th>이름</th>
                                                        <th>성별</th>
                                                        <th>아이디</th>
                                                        <th>연락처</th>
                                                        <th>주소</th>
                                                        <th>가입일</th>
                                                        <th>비고</th>
                                                    </tr>
                                                </thead>
                                                <tbody
                                                    class="userList"
                                                    id="waitingUserList"
                                                ></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
            <%- include('footer.ejs') %>
        </div>

        <!-- Scripts -->
        <script src="./js/jquery.min.js"></script>
        <script src="./js/browser.min.js"></script>
        <script src="./js/breakpoints.min.js"></script>
        <script src="./js/util.js"></script>
        <script src="./js/main.js"></script>
        <script src="./js/tabmenu.js"></script>
        <script src="./js/admin.js"></script>
    </body>
</html>
